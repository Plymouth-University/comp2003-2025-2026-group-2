{
	"name": "logsmart",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"logs": "wrangler pages deployment tail --project-name=logsmart",
		"deploy": "svelte-kit sync && bun run build && wrangler pages deploy .svelte-kit/cloudflare --project-name=logsmart",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"gen:api": "openapi-typescript openapi.json -o src/lib/api-types.d.ts",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.49.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^20.19.25",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"openapi-typescript": "^7.10.1",
		"prettier": "^3.7.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.45.2",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.0",
		"vite": "^7.2.6",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"dependencies": {
		"@neodrag/svelte": "^2.3.3",
		"openapi-fetch": "^0.15.0"
	}
}
